    "axios": "^1.12.2",
    "dotenv": "^16.4.5",
    "express": "^4.19.2",
    "multer": "^2.0.2",
    "mysql2": "^3.6.5",
    "nanoid": "^3.3.7",
    "node-fetch": "^3.3.2",
    "nodemon": "^3.0.2",
    "npm-run-all": "^4.1.5",
    "telegraf": "^4.16.3",
    "ts-node": "^10.9.2",
    "typescript": "^5.3.3"
  }
